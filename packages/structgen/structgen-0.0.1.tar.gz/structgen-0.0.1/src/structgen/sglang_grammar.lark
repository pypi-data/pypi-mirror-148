spec: module constants tables

?module: "module" CNAME ";"

constants: constant*
constant: "const" CNAME "=" INT ";"

tables: table*
table: "table" CNAME "{" columns+ "}"

columns: column ("," column)*
column: CNAME type
?type: scalartype | arraytype
scalartype: typename
arraytype: typename "[" CNAME "]"

!typename: "u8" | "u16" | "u32" | "u64"
         | "i8" | "i16" | "i32" | "i64"

%import common (INT, CNAME, WS)
%ignore WS
